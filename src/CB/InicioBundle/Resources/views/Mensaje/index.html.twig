{% block form %}
    <h1>Mensaje list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>Nombre</th>
                <th>Asunto</th>
                <th>Contenido</th>
                <th>Leido</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('mensaje_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.email }}</td>
                <td>{{ entity.nombre }}</td>
                <td>{{ entity.asunto }}</td>
                <td>{{ entity.contenido }}</td>
                <td>{{ entity.leido }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('mensaje_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('mensaje_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('mensaje_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
